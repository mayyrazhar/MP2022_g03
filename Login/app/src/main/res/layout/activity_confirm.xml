<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A6BDBB"
    tools:context=".confirmActivity">

    <TextView
        android:id="@+id/tvh_fullname"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Name:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.120000005" />

    <TextView
        android:id="@+id/tvh_carType"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Car Type:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_phoneNumber"
        app:layout_constraintVertical_bias="0.05" />

    <TextView
        android:id="@+id/tvh_ic"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="IC Number:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_fullname"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/tvh_phoneNumber"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Phone Number:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_ic"
        app:layout_constraintVertical_bias="0.037" />

    <TextView
        android:id="@+id/tvh_carName"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Car Name:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_carType"
        app:layout_constraintVertical_bias="0.047" />

    <TextView
        android:id="@+id/tvh_reserveDate"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Reserve Date:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_carName"
        app:layout_constraintVertical_bias="0.01999998" />

    <TextView
        android:id="@+id/tvh_returnDate"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Return Date:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_reserveDate"
        app:layout_constraintVertical_bias="0.050000012" />

    <Button
        android:id="@+id/btnh_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_price"
        app:layout_constraintVertical_bias="0.337" />

    <TextView
        android:id="@+id/tvh_price"
        android:layout_width="324dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_prosemibold"
        android:text="Total: RM"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvh_returnDate"
        app:layout_constraintVertical_bias="0.060000002" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sourcesans_problack"
        android:text="CONFIRMATION ORDER"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112" />

</androidx.constraintlayout.widget.ConstraintLayout>